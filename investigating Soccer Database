    "merge_comb1=pd.merge(team_copy,Match_copy,left_on='team_api_id',right_on='home_team_api_id')\n"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 32,
   "metadata": {},
   "outputs": [],
   "source": [
    "merge_comb2=pd.merge(team_copy,Match_copy,left_on='team_api_id',right_on='away_team_api_id')\n"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 34,
   "metadata": {},
   "outputs": [],
   "source": [
    "home_goals=merge_comb1.groupby(\"team_long_name\")['home_team_goal'].sum()\n",
    "away_goals=merge_comb2.groupby(\"team_long_name\")['away_team_goal'].sum()"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 54,
   "metadata": {},
   "outputs": [],
   "source": [
    "most_goals=home_goals+away_goals\n",
    "ascending=most_goals.sort_values(ascending=False).head(10)\n"
   ]
  },
